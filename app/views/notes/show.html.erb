<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @note.title %>
</p>

<p>
  <strong>Content:</strong>
  <%= @note.content %>
</p>


<div id= "like-link">
  <%= render 'likes/like_link', note: @note %>
  </div>
  </br>
  </br>
<div>
  <small>
    <%= link_to @note.likes.count, liking_users_note_path(@note.id), id: "like-count" %>people liked this post.
    
  </small>
</div>

<% if current_user?(@note.user) %>
<%= link_to 'Edit', edit_note_path(@note) %> |
<%= link_to 'Back', notes_path %>
<%end%>
